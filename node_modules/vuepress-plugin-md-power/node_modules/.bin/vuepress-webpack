#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Code Web/Frozen-Docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepr_3d4da7ebca8501c33c86ccb891369f33/node_modules/vuepress/bin/node_modules:/mnt/e/Code Web/Frozen-Docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepr_3d4da7ebca8501c33c86ccb891369f33/node_modules/vuepress/node_modules:/mnt/e/Code Web/Frozen-Docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepr_3d4da7ebca8501c33c86ccb891369f33/node_modules:/mnt/e/Code Web/Frozen-Docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Code Web/Frozen-Docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepr_3d4da7ebca8501c33c86ccb891369f33/node_modules/vuepress/bin/node_modules:/mnt/e/Code Web/Frozen-Docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepr_3d4da7ebca8501c33c86ccb891369f33/node_modules/vuepress/node_modules:/mnt/e/Code Web/Frozen-Docs/node_modules/.pnpm/vuepress@2.0.0-rc.26_@vuepr_3d4da7ebca8501c33c86ccb891369f33/node_modules:/mnt/e/Code Web/Frozen-Docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vuepress@2.0.0-rc.26_@vuepr_3d4da7ebca8501c33c86ccb891369f33/node_modules/vuepress/bin/vuepress-webpack.js" "$@"
else
  exec node  "$basedir/../../../../../vuepress@2.0.0-rc.26_@vuepr_3d4da7ebca8501c33c86ccb891369f33/node_modules/vuepress/bin/vuepress-webpack.js" "$@"
fi
